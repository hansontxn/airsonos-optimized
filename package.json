{
  "name": "airsonos-optimized",
  "version": "0.3.0",
  "author": "Stephen Wan <stephen@stephenwan.net>",
  "description": "High-performance AirPlay to Sonos bridge with Home Assistant integration, adaptive buffering, and intelligent optimization",
  "homepage": "https://github.com/HansonTan/airsonos-optimized",
  "contributors": [
    {
      "name": "Stephen Wan",
      "email": "stephen@stephenwan.net"
    },
    {
      "name": "AirSonos Optimized Contributors",
      "url": "https://github.com/HansonTan/airsonos-optimized/graphs/contributors"
    }
  ],
  "scripts": {
    "start": "node ./lib/index.js",
    "start:dev": "NODE_ENV=development node ./lib/index.js",
    "start:prod": "NODE_ENV=production node ./lib/index.js",
    "build": "node scripts/build.js",
    "build:clean": "npm run clean && npm run build",
    "clean": "node scripts/clean.js",
    "test": "jest",
    "test:unit": "jest test/unit_tests.js",
    "test:integration": "jest test/integration_tests.js",
    "test:performance": "jest test/performance_benchmark.js --testTimeout=60000",
    "test:compatibility": "jest test/device_compatibility.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:all": "./scripts/run_tests.sh all",
    "lint": "eslint src/ lib/ test/ scripts/ --ext .js",
    "lint:fix": "eslint src/ lib/ test/ scripts/ --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,json,md,yml,yaml}\"",
    "validate": "./scripts/validate_config.js",
    "validate:config": "./scripts/validate_config.js config/options.json",
    "verify": "node scripts/verify_installation.js",
    "verify:full": "node scripts/verify_installation.js --full",
    "health-check": "node ./lib/health-check.js",
    "monitor": "node --experimental-worker ./lib/monitor.js",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "postinstall": "node scripts/post_install.js",
    "preuninstall": "node scripts/cleanup.js --pre-uninstall",
    "uninstall": "node scripts/cleanup.js --uninstall",
    "release:prepare": "node scripts/prepare_release.js",
    "release:validate": "node scripts/validate_structure.js",
    "hacs:validate": "node scripts/validate_hacs.js",
    "docker:build": "docker build -t airsonos-optimized .",
    "docker:run": "docker run -p 5000:5000 -p 8099:8099 airsonos-optimized"
  },
  "bin": {
    "airsonos-optimized": "./lib/index.js",
    "airsonos": "./lib/index.js"
  },
  "main": "./lib/index.js",
  "files": [
    "lib/",
    "src/",
    "config/",
    "translations/",
    "scripts/validate_config.js",
    "scripts/verify_installation.js",
    "scripts/cleanup.js",
    "scripts/post_install.js",
    "README.md",
    "CHANGELOG.md",
    "DOCS.md",
    "LICENSE",
    "config.yaml",
    "apparmor.txt",
    "run.sh"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/HansonTan/airsonos-optimized.git"
  },
  "bugs": {
    "url": "https://github.com/HansonTan/airsonos-optimized/issues"
  },
  "dependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "express": "^4.18.2",
    "ws": "^8.14.0",
    "winston": "^3.8.2",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.2",
    "pidusage": "^3.0.2",
    "prom-client": "^14.2.0",
    "ip": "^2.0.0",
    "flags": "^0.1.3",
    "bluebird": "^3.7.2",
    "nicercast": "^0.1.0",
    "nodetunes": "^0.3.0",
    "sonos": "^1.15.0",
    "js-yaml": "^4.1.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/node": "^7.22.0",
    "jest": "^29.7.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.0",
    "jscs": "^2.1.0",
    "@types/node": "^20.8.0"
  },
  "peerDependencies": {
    "node": ">=16.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64",
    "arm"
  ],
  "license": "MIT",
  "keywords": [
    "airplay",
    "airtunes",
    "sonos",
    "home-assistant",
    "hacs",
    "audio",
    "streaming",
    "bridge",
    "wireless",
    "music",
    "optimization",
    "performance",
    "addon",
    "hassio",
    "supervisor"
  ],
  "hacs": {
    "name": "AirSonos Optimized",
    "category": "addon",
    "description": "High-performance AirPlay to Sonos bridge with Home Assistant integration",
    "documentation": "https://github.com/HansonTan/airsonos-optimized/blob/master/README.md"
  },
  "homeAssistant": {
    "addons": true,
    "category": "Media",
    "supported_arch": [
      "amd64",
      "armv7",
      "aarch64"
    ]
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/HansonTan"
  }
}
